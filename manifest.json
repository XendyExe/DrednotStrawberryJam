{
    "name": "Drednot Strawberry Jam Mod",
    "description": "totally not stolen from the 2020 Strawberry Jam Collab from celeste",
    "version": "182",
    "author": "Xendy",
    "permissions": ["storage"],
    "web_accessible_resources": [
      {
        "matches": ["*://*.drednot.io/*"],
        "resources": [
          "scripts/*",
          "audio/*",
          "mods/*"
        ]
      }
    ],
    "content_scripts": [
      {
        "matches": ["*://*.drednot.io/*"],
        "js": ["scripts/content-script.js"],
        "run_at" : "document_start",
        "all_frames": true
      },
      {
        "matches": ["*://*.drednot.io/*"],
        "js": ["scripts/contentLoad.js"],
        "all_frames": true
      }
    ],
    "manifest_version": 3
  }